{
  "expo": {
    "jsEngine": "hermes",
    "name": "Courrier",
    "slug": "courrier",
    "version": "1.0.0",
    "icon": "./assets/iconArch.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashArchive.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mediabox.cra"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mediabox.courrier"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/iconArch.png",
      "color": "#dddddd",
      "androidMode": "default",
      "androidCollapsedTitle": "Cra",
      "iosDisplayInForeground": true
    },
    "scheme": "cra",
    "description": "Une application pour remplir le compte rendu d'activite de mediabox"
  }
}
